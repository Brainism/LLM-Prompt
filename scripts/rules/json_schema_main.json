{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Event Extraction",
  "type": "object",
  "required": [
    "title",
    "date",
    "location",
    "involved",
    "summary"
  ],
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1
    },
    "date": {
      "type": "string",
      "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
    },
    "location": {
      "type": "string",
      "minLength": 1
    },
    "involved": {
      "type": "string",
      "minLength": 1
    },
    "summary": {
      "type": "string",
      "minLength": 1
    }
  },
  "additionalProperties": false
}
